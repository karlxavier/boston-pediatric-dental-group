<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>Customers</h2>
    <ol class="breadcrumb">
      <li>
        <%= link_to 'Dashboard', admin_root_path %>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">

  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">

      <%= flash_messages %>

      <div class="ibox float-e-margins" style="font-size: 13px;">
        <div class="ibox-title">
          <h5></h5>
          <div class="ibox-tools">

          </div>
        </div>
        <div class="ibox-content">
          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dataTables-list dataTable">
              <thead>
              <tr role="row">
                <th>Template</th>
                <th></th>
              </thead>
              <tbody>
              <% DefaultTemplate.all.each do |default_template| %>
                <tr>
                  <td><%= default_template.template_type %></td>
                  <td>
                    <div class="btn-group">
                      <%= link_to 'Edit', edit_admin_default_template_path(default_template), class: "btn btn-xs btn-white" %>
                    </div>
                  </td>
                </tr>
              <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>