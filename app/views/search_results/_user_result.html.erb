<div class="row">
  <% results.each do |result| %>
    <div class="col-md-4">
      <div class="card flex-row mb-4 box-shadow h-md-250" style="box-shadow: 0 5px 15px rgba(33, 33, 33, .4);" >
        <img class="card-img-left flex-auto d-none d-lg-block" src="/assets/avatar-1.jpg" />
          <div class="card-body d-flex flex-column align-items-start">
            <div class="col">
              <strong><%= result.full_name %></strong>
              <p>
                <%= result.email %>
              </p>
            </div>
          </div>
      </div>
    </div>
  <% end %>
</div>