<div id="order_messages">
	<div class="dropzonable" data-behavior='chatroom-orders' data-chatroom-id='<%= active_room.id %>' id="chatroom_msgs">
		<% messages.each do |message| %>
			<div class="widget-chat-item with-media left">
				<div class="widget-chat-info">
		 			<%= render '/messages/message', message: message, current_user_id: current_user.id %>
				</div>
			 </div>
		<% end %>
		<input type="hidden" id="current_user_id" value="<%= current_user.id %>">
	</div>
</div>

