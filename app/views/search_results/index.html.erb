<%= render "form" %>
<div class="row">
  <div class="col">
    <% if  @results.each do |key, results| %>
      <strong><%= key.to_s.pluralize(results.size) %> that match "<%= params[:search_term] %>"</strong>
      <%= render "#{key.downcase}_result", results: results %>
    <% end.empty? %>
      Nothing found
    <% end %>
  </div>
</div>