<strong>Users that match "<%= params[:search_term] %>"</strong>
<%= render "see_more", filter_by: "user" if show_see_more %>
<div class="row">
  <% results.each do |result| %>
    <div class="col-md-4">
      <div class="card flex-row mb-4 h-md-250">
        <img class="card-img-left flex-auto d-none d-lg-block" src="/assets/avatar-1.jpg" height="91" width="91" />
          <div class="card-body d-flex flex-column align-items-start">
            <div class="col">
              <strong><%= result.full_name %></strong>
              <p>
                <%= result.email %>
              </p>
            </div>
          </div>
      </div>
    </div>
  <% end %>
</div>